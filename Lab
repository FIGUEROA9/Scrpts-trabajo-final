create TABLE if not EXISTS clientes(
    id serial primary key,
    nombre varchar(100)not null,
    email varchar(100),
    fecha_registro date default current_date
);

insert into clientes (nombre, email) values ('Juan Perez', 'juanpe@gmail.com');
insert into clientes (nombre, email) values ('Maria Gomez', 'mariagomez@gmail.com');
